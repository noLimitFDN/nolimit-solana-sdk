{
  "name": "@nolimit/solana-sdk",
  "version": "1.3.1",
  "description": "Solana SDK for noLimit Foundation - Privacy-first AI, Swap, and Mixer utilities with x402 payment protocol",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./chat": "./dist/clients/chat.js",
    "./swap": "./dist/clients/swap.js",
    "./mixer": "./dist/clients/mixer.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/clients/*.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc --out docs/api src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nolimit",
    "privacy",
    "ai",
    "solana",
    "x402",
    "defi",
    "swap",
    "mixer",
    "uncensored",
    "jupiter",
    "spl-token"
  ],
  "author": "noLimit Foundation <dev@nolimit.foundation>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/noLimitFDN/nolimit-solana-sdk"
  },
  "homepage": "https://nolimit.foundation/integration",
  "bugs": {
    "url": "https://github.com/noLimitFDN/nolimit-solana-sdk/issues"
  },
  "dependencies": {
    "x402-solana": "^0.1.5"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0",
    "@solana/spl-token": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

